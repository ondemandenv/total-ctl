{
  "name": "extract-env",
  "version": "1.0.0",
  "private": true,
  "description": "Map branch to environment/account/region with strong typing",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "dev": "ts-node src/index.ts",
    "debug:ts": "node --inspect-brk -r ts-node/register src/index.ts",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}



